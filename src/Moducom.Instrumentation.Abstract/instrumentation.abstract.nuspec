<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- Required elements-->
    <id>Moducom.Instrumentation.Abstract</id>
    <version>0.9.0-alpha002</version>
    <description>Core instrumentation abstractions for sprinkling throughout your code</description>
    <authors>Malachi Burke</authors>

    <!-- Optional elements -->
    <releaseNotes>Won't bump to 1.0.0 until TC is auto versioning the builds for us</releaseNotes>
    <dependencies>
      <dependency id="Fact.Extensions.Collection" version="1.0.0-beta002" />
    </dependencies>
    <!-- ... -->
  </metadata>
  <!-- Optional 'files' node -->
  <files>
    <file src="bin/Debug/net46/Moducom.Instrumentation.Abstract.dll" target="lib\net46" />
    <file src="bin/Debug/net46/Moducom.Instrumentation.Abstract.pdb" target="lib\net46" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.dll" target="lib\netstandard1.1" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.pdb" target="lib\netstandard1.1" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.dll" target="lib\netstandard1.3" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.pdb" target="lib\netstandard1.3" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.dll" target="lib\netstandard2.0" />
    <file src="bin/Debug/netstandard1.6/Moducom.Instrumentation.Abstract.pdb" target="lib\netstandard2.0" />
    <!-- experimentally adding cs files also, due to TeamCity TW-32067 -->
    <file src="*.cs" />
    <file src="bin/Debug/net46/*.xml" />
    <file src="bin/Debug/**/*.xml" />
